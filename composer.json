{
  "name": "cimmwolf/utilities",
  "description": "Utilities for web development",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Denis Beliaev",
      "email": "cimmwolf@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "DenisBeliaev\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "cimmwolf/mdash",
        "version": "dev-master",
        "dist": {
          "url": "https://github.com/cimmwolf/mdash/archive/master.zip",
          "type": "zip"
        },
        "autoload": {
          "classmap": [
            "src-php/EMT.php"
          ]
        }
      }
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "cimmwolf/mdash": "dev-master",
    "symfony/finder": "^5.0",
    "symfony/filesystem": "^5.0",
		"ext-gd": "*"
	},
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  }
}
